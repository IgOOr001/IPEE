programa
{
	funcao inicio ()
	{
		inteiro primeiroNumero,segundoNumero
		
          escreva("Contagem com o limite de 2 bits:\n")
			
		   para(primeiroNumero=0;primeiroNumero<=1;primeiroNumero++)
		    {
		      para(segundoNumero=0;segundoNumero<=1;segundoNumero++)
		   	   {  
	   	  		 escreva(primeiroNumero,primeiroNumero,"\n")
	   	  		 	   	
		   	   }
		    }		
	}
}
